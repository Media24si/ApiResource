{
  "name": "media24si/api-resource",
  "description": "Laravel Guzzle Api Resource Package",
  "license": "MIT",
  "keywords": ["laravel", "guzzle", "rest client"],
  "homepage": "https://github.com/Media24si/ApiResource",
  "authors": [
    {
      "name": "Bostjan Oblak",
      "email": "bostjan@muha.cc"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Media24si\\ApiResource\\": "src/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Media24si\\ApiResource\\ApiResourceServiceProvider"
          ],
          "aliases": {
              "Api": "Media24si\\ApiResource\\Facades\\ApiResource"
          }
      }
  }
}
